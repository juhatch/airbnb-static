<div class="container">
  <div class="row">
    <div class="col-xs-12">
      <h2 class="text-center marged">OUR TEAM</h2>
      <ul class="list-inline text-center padded">
        <% ["julien", "audrey"].each do |member| %>
          <li>
            <%= image_tag "#{member}.png", class: "img-circle avatar avatar-large" %>
            <h3><%= member.capitalize %></h3>
          </li>
        <% end %>
      </ul>
      <h3 class="padded text-center">

        <% link_to "mailto:contact@airbnb.com", class: "airbnb-contact" do %>
          <i class="fa fa-envelope"></i>
          contact@airbnb.com
        <% end %>

      </h3>
    </div>
  </div>
</div>
